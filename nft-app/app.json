{
  "expo": {
    "name": "nft-app",
    "slug": "NFT-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo-Compressed.png",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#151F28"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo-Compressed.png",
        "backgroundColor": "#151F28"
      },
      "package": "nftapp.apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
